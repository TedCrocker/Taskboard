<div class="issue {color} {stateClass} commonStyles" id="issue-{Id}">
	<textarea class="content">{content}</textarea>
	<button class="openIssue">Open</button>
	<button class="closeIssue">Close</button>
	<button class="deleteIssue">X</button>
	<button class="unopenIssue">Clear</button>	
	<div class="assignedTo">{assignedTo}</div>
</div>